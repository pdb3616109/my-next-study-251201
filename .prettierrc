{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "commaStyle": "last",
  "braceStyle": "1tbs",
  "spaceBeforeFunctionParen": true,
  "spaceInfixOps": true,
  "keywordSpacing": true,
  "indent": 2
}